      <conf_evalphp><![CDATA[if ( $save == 1)
{
	if ( is_array($_POST['perscom_offline_access_groups']) )
	{
		$_POST['perscom_offline_access_groups'] = ',' . implode(",",$_POST['perscom_offline_access_groups']) . ',';
	}
	else
	{
		$_POST['perscom_offline_access_groups'] = "";
	}

	$key = 'perscom_offline_access_groups';
}

if ( $show == 1 )
{
